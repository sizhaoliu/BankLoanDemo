    <sequenceFlow id="_3-_4" sourceRef="_3" targetRef="_4" />
    <sequenceFlow id="_4-_5" sourceRef="_4" targetRef="_5" name="otherwise" tns:priority="100" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.jboss.org/drools/rule" >eval(true)</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_4-_7" sourceRef="_4" targetRef="_7" name="rejected" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.jboss.org/drools/rule" >LoanSimulator(rejected == true)</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_5-_8" sourceRef="_5" targetRef="_8" />
    <sequenceFlow id="_8-_10" sourceRef="_8" targetRef="_10" name="rejected" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.jboss.org/drools/rule" >LoanSimulator(rejected == true)</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_18-_11" sourceRef="_18" targetRef="_11" />
    <sequenceFlow id="_11-_12" sourceRef="_11" targetRef="_12" />
    <sequenceFlow id="_12-_13" sourceRef="_12" targetRef="_13" name="otherwise" tns:priority="100" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.jboss.org/drools/rule" >eval(true)</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_12-_16" sourceRef="_12" targetRef="_16" name="rejected" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.jboss.org/drools/rule" >LoanSimulator(rejected == true)</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_8-_18" sourceRef="_8" targetRef="_18" name="otherwise" tns:priority="100" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.jboss.org/drools/rule" >eval(true)</conditionExpression>
    </sequenceFlow>

