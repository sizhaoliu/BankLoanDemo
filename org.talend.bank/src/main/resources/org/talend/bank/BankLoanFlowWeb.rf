  <escalation id="InvalidAge" escalationCode="InvalidAge" />
  <escalation id="LowDeposit" escalationCode="LowDeposit" />
  <escalation id="LowPayment" escalationCode="LowPayment" />
  <process processType="Private" isExecutable="true" id="loanflowweb" name="flow" tns:packageName="org.talend.bank" >

    <extensionElements>
     <tns:import name="org.talend.bank.LoanSimulator" />
     <tns:import name="org.talend.bank.LoanCalculator" />
    </extensionElements>
    <!-- nodes -->
    <startEvent id="_1" name="Start" />
    <endEvent id="_2" name="End" >
        <terminateEventDefinition/>
    </endEvent>
    <businessRuleTask id="_3" name="Validate Age" g:ruleFlowGroup="AgeValidation" >
    </businessRuleTask>
    <exclusiveGateway id="_4" name="Gateway" gatewayDirection="Diverging" />
    <businessRuleTask id="_5" name="Check Deposit" g:ruleFlowGroup="DepositCheckWeb" >
    </businessRuleTask>
    <endEvent id="_7" name="Error" >
      <escalationEventDefinition escalationRef="InvalidAge" />
    </endEvent>
    <exclusiveGateway id="_8" name="Gateway" gatewayDirection="Diverging" />
    <endEvent id="_10" name="Error" >
      <escalationEventDefinition escalationRef="LowDeposit" />
    </endEvent>
    <businessRuleTask id="_11" name="Check Salary" g:ruleFlowGroup="SalaryCheckGroup" >
    </businessRuleTask>
    <exclusiveGateway id="_12" name="Gateway" gatewayDirection="Diverging" />
    <scriptTask id="_13" name="Process Loan" >
      <script>System.out.println("Loan Approuved.");
System.out.println("Prepare documents, Arrange a meeting with the customer ...");</script>
    </scriptTask>
    <endEvent id="_16" name="Error" >
      <escalationEventDefinition escalationRef="LowPayment" />
    </endEvent>
    <businessRuleTask id="_18" name="Calculate Monthly Payment" g:ruleFlowGroup="MonthlyPaymentCalculationGroup" >
    </businessRuleTask>

    <!-- connections -->
    <sequenceFlow id="_13-_2" sourceRef="_13" targetRef="_2" />
